{
    "name": "dynamic/foxystripe-inventory",
    "description": "simple inventory and embargo add-on for FoxyStripe",
    "authors": [
        {
            "name": "Dynamic, Inc.",
            "email": "dev@dy.ag"
        }
    ],
    "keywords": [
        "silverstripe", "foxystripe", "ecommerce", "inventory"
    ],
    "type": "silverstripe-module",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/cms": "^3.2",
        "silverstripe/framework": "^3.2",
        "dynamic/foxystripe": "^2.0",
        "unclecheese/display-logic": "^1.4"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^3.7"
    },
    "config": {
        "process-timeout": 600
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-name": "foxystripe-inventory",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
